{
	// 应用名称，表示该插件用在什么应用里的
	"app": "layout",
	// 插件名称，插件的唯一标识
	"name": "editor",
	// 插件标题，用于检索和识别
	"title": "示例插件",
	// 插件描述，介绍改插件是做什么用的
	"description": "描述该插件是做什么用的",
	// 插件版本号，用于升级迭代
	"version": "1.0",
	// 插件语言包文件路径
	"lang_path": "./lang/",
	// 插件图标
	"icon": "/layout/editor/img/logo.png",
	// 插件指令，通过指令方式调用插件
	"cmd": "layout.editor",
	// 排序，表示多个插件时在应用下执行的先后顺序
	"sort": 10
}
