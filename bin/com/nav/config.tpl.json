{
	// 路由导航名称
	"name": "admin",
	// 执行排序, 数值越小越优先执行
	"sort": 1,
	/* 路由数组 */ 
	"routes": [{
		// 路由名称
		"name": "root",
		// 路由路径
		"path": "/",
		// vue组件路径
		"component": "",
		// 访问级别
		"level": 1,
		/* 授权协议 */ 
		"oauth": {
			// 是否需要登录, true表示需要登录才能访问该接口
			"signIn": false,
			// 会员权限级别, 如果小于该值, 则无法访问, 0为不限制
			"vip": 0,
			// 管理员权限级别, 如果小于该值, 则无法访问, 0为不限制
			"gm": 0,
			// 商户/第三方权限级别, 如果小于该值, 则无法访问, 0为不限制
			"mc": 0,
			// 用户组, 允许访问该接口的普通用户群体, 例如: [1,3]
			"user_group": [1],
			// 管理组, 允许访问该接口的管理员群体, 例如: [1,3]
			"user_admin": [1]
		}
	}],
	/* 顶部导航 */ 
	"top": [],
	/* 左边导航 */ 
	"left": [],
	/* 底部导航 */
	"bottom": [],
	/* 右边导航 */
	"right": [],
	/* 快捷导航 */
	"quick": [],
	/* 桌面导航 */
	"desktop": [],
	/* 主导航 */
	"main": [
		{
			// 导航标题
			"title": "系统",
			// 路由地址
			"url": "/sys",
			// 显示顺序, 数字越小显示越靠前
			"display": 3,
			/* 子导航 */
			"sub": [{
					// 导航标题
					"title": "用户管理",
					// 路由地址
					"url": "/sys/app"
				},
				{
					// 导航标题
					"title": "网站设置",
					// 路由地址
					"url": "/sys/web"
				},
				{
					// 导航标题
					"title": "应用管理",
					// 路由地址
					"url": "/sys/app"
				}
			]
		}]
	}]
}