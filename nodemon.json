{
	"restartable": "rs",
	"ignore": [
		".git",
		".svn",
		"node_modules/**/node_modules",
		"*/static/img/*",
		"*/static/js/*",
		"*/static/src/*",
		"*/static/css/*"
	],
	"verbose": true,
	"execMap": {
		"": "node",
		"js": "node --harmony"
	},
	"watch": [
		
	],
	"env": {
		"NODE_ENV": "development"
	},
	"ext": "js ts json",
	"legacy-watch": false
}
