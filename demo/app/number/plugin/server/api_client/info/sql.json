{
    "name": "number_info",
    "title": "号码信息",
    "table": "number_info",
    "key": "number_id",
    "orderby_default": "`display` asc",
    "field_obj": "`number_id`,`private`,`heYue`,`hide`,`kid`,`yid`,`tid`,`cityID`,`gid`,`sid`,`uid`,`rid`,`average`,`score`,`huaFei`,`maiJia`,`daiLiJia`,`diJia`,`order`,`hot`,`collection`,`createTime`,`topTime`,`updateTime`,`number`,`format`,`activity`,`group`,`desc`,`diXiao`,`log`,`note`",
    "field_default": "`number_id`,`private`,`heYue`,`hide`,`kid`,`yid`,`tid`,`cityID`,`gid`,`sid`,`uid`,`rid`,`average`,`score`,`huaFei`,`maiJia`,`daiLiJia`,`diJia`,`order`,`hot`,`collection`,`display`,`createTime`,`topTime`,`updateTime`,`number`,`format`,`activity`,`group`,`desc`,`diXiao`",
    "method": "get",
    "query": {
        "number": "`number` like '%{0}%'",
        "huaFei_min": "`huaFei` >= {0}",
        "huaFei_max": "`huaFei` <= {0}",
        "maiJia_min": "`maiJia` >= {0}",
        "maiJia_max": "`maiJia` <= {0}",
        "daiLiJia_min": "`daiLiJia` >= {0}",
        "daiLiJia_max": "`daiLiJia` <= {0}",
        "diJia_min": "`diJia` >= {0}",
        "diJia_max": "`diJia` <= {0}",
        "format": "`format` like '%{0}%'",
        "order_min": "`order` >= {0}",
        "order_max": "`order` <= {0}",
        "activity": "`activity` like '%{0}%'",
        "hot_min": "`hot` >= {0}",
        "hot_max": "`hot` <= {0}",
        "collection_min": "`collection` >= {0}",
        "collection_max": "`collection` <= {0}",
        "display_min": "`display` >= {0}",
        "display_max": "`display` <= {0}",
        "createTime_min": "`createTime` >= '{0}'",
        "createTime_max": "`createTime` <= '{0}'",
        "topTime_min": "`topTime` >= '{0}'",
        "topTime_max": "`topTime` <= '{0}'",
        "updateTime_min": "`updateTime` >= '{0}'",
        "updateTime_max": "`updateTime` <= '{0}'",
        "group": "`group` like '%{0}%'",
        "diXiao": "`diXiao` like '%{0}%'",
        "log": "`log` like '%{0}%'",
        "note": "`note` like '%{0}%'",
        "score_min": "`score` >= {0}",
        "score_max": "`score` <= {0}",
        "average_min": "`average` >= {0}",
        "average_max": "`average` <= {0}",
        "zhongFormat": "`format` like '%/*{0}*/%'",
        "weiFormat": "`format` like '%/*{0}/%'",
        "specialFormat": "`format` like '%/{0}/%'",
        "cycleFormat": "`format` like '%/**{0}/%'",
        "segment": "`number` like '{0}%'",
        "max": "`maiJia` <= {0}",
        "min": "`maiJia` >= {0}",
        "weiShu": "`number` like '%{0}'",
        "repeatNum": "length(`number`) - length(REPLACE(`number`,{0},'')) > 4",
        "birthday": "`number` like '%{0}%' and `format` like '%生日%'",
        "baoHan": "`number` like '%{0}%'",
        "buBaoHan": "`number` REGEXP '^[^{0}]+$'",
        "weiZhi": "`number` like '%{0}%'",
        "order_no": "`order` != {0}",
        "onlyNum": "substring(`number`, 3) REGEXP '^[{0}]+$'",
        "gongHuoShang": "`gid` in (select x.uid FROM mm_user as x WHERE x.name like '%{0}%')",
        "dateDiff": "DATEDIFF(`updateTime`, NOW()) = {0}",
        "recommend": "`rid` in (select x.rid FROM mm_number_recommend as x WHERE x.title like '%{0}%')",
        "no_rid": "`rid` != {0}",
        "cityID_min": "`cityID` >= {0}",
        "cityID_max": "`cityID` <= {0}",
        "desc": "`desc` like '%{0}%'"
    },
    "query_default": {
        "order": "`order` < 3"
    },
    "update": {
        "cityID_add": "`cityID` = `cityID` + {0}",
        "huaFei_add": "`huaFei` = `huaFei` + {0}",
        "maiJia_add": "`maiJia` = `maiJia` + {0}",
        "daiLiJia_add": "`daiLiJia` = `daiLiJia` + {0}",
        "diJia_add": "`diJia` = `diJia` + {0}",
        "order_add": "`order` = `order` + {0}",
        "hot_add": "`hot` = `hot` + {0}",
        "collection_add": "`collection` = `collection` + {0}",
        "display_add": "`display` = `display` + {0}",
        "score_add": "`score` = `score` + {0}",
        "average_add": "`average` = `average` + {0}"
    },
    "format": [
        {
            "key": "private",
            "title": "是否私有",
            "list": [
                "否",
                "是"
            ]
        },
        {
            "key": "heYue",
            "title": "是否合约号",
            "list": [
                "否",
                "是"
            ]
        },
        {
            "key": "hide",
            "title": "是否隐藏",
            "list": [
